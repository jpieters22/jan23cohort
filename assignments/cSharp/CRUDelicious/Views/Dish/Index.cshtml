@model List<Dish>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/chef.css">
    <title>Home</title>
</head>
<body>
    <h1>Welcome to CRUDelicious!</h1>
    <nav>
        <a href="/">Home</a>
        <a href="/dishes/new">Add a Dish</a>
    </nav>
    <div>
        <h2>Check out some recent dishes!</h2>
        @foreach (var Dish in Model)
        {
            <div>
            <a asp-controller="Dish" asp-action="ViewRecipe" asp-route-DishId="@Dish.DishId">@Dish.Name</a>
            <p>by @Dish.Chef</p>
            </div>
        }
    </div>
</body>
</html>
